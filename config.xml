<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns       = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.someone.example"
        versionCode = "10" 
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>PhoneGap Example</name>
    <description>An example for phonegap build docs.</description>
    <author href="https://build.phonegap.com" email="support@phonegap.com">rbtx99 the coder</author>

	<!-- <gap:platform name="ios" /> -->
	<gap:platform name="android" />
	<!-- <gap:platform name="webos" />
	<gap:platform name="symbian" />
	<gap:platform name="blackberry" />
	<gap:platform name="winphone" /> -->
	
	<preference name="orientation" value="landscape" />
	<preference name="fullscreen" value="true" />
	<icon src="icon.png" />
	<gap:splash src="splash/ios/splash.png" gap:platform="android" />
	
	
	<gap:plugin name="com.randdusing.bluetoothle" version="1.0.0" />
	
	
</widget>